{
  "name": "unilint",
  "version": "1.0.0",
  "description": "自动生成配置文件",
  "main": "./dist/index.cjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "unbuild": {
    "failOnWarn": false
  },
  "files": ["bin", "dist", "package.json"],
  "bin": {
    "un": "./bin/un.js"
  },
  "homepage": "https://github.com/ALiangTech/unilint",
  "scripts": {
    "json": "node ./src/config/index.mjs",
    "build": "pnpm run json && unbuild"
  },
  "keywords": ["eslint", "stylelint", "preitter", "vue", "alloy"],
  "author": "AliangTech",
  "license": "ISC",
  "dependencies": {
    "commander": "^10.0.1",
    "inquirer": "^8.0.0",
    "progress": "^2.0.3",
    "read-package-json": "^6.0.4"
  },
  "devDependencies": {
    "glob": "^10.2.7"
  }
}
